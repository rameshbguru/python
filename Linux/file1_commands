(13)rmdir
(14)rm -rf <dir>
(15)cp <source file> <destination file>
(16)cp -r <source directory> <destination directory>
(17)mv <old file> <new file>  [note:In current directory]
(18)mv <old file> <new directory>  [note:In current directory]
(19)mv <file name> <path>    [note:move to different directory]
(20)mv <directory name> <path>  [note:move to different directory]
(21)ls
(22)ls -l
(23)ls -ltr
(24)ls -ld
(25)history
(26)diff
