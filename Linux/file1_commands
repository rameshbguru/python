(28)echo *
(29)env
(30)export PATH=$PATH:<path>
(31)echo $PATH
(32)echo $USER
(33)echo $HOME
(34)echo $SHELL
(35)hostname
(36)hostname -I
(37)hostname -f
(38)ifconfig
(39)route -n
(40)ping <ip address>
(41)ping google.com
(42)ssh <username>@<ip address>
(43)scp <file name> <user name>@<ip address>:<path>
(44)scp -r <file name> <user name>@<ip address>:<path>
(45)netstat -ntalp |grep <port/service name>
(46)nc -vz <ip address> <port>
(47)telnet <ip address>:<port>
(48)finger <user name>
